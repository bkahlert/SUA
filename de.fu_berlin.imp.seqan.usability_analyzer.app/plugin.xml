<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.4"?>

<plugin>
   <extension point="org.eclipse.help.contexts">
      <contexts file="contexts.xml">
      </contexts>
   </extension>
   <extension point="org.eclipse.ui.commands">
      <command id="de.fu_berlin.imp.seqan.usability_analyzer.commands.Empty" name="EMPTY">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.fu_berlin.imp.seqan.usability_analyzer.Perspective"
            id="de.fu_berlin.imp.seqan.usability_analyzer.app.perspective"
            name="SeqAn Usability Analyzer">
      </perspective>
   </extension>
   <extension
         id="de.fu_berlin.imp.seqan.usability_analyzer.application"
         point="org.eclipse.core.runtime.applications">
      <application
            cardinality="singleton-global"
            thread="main"
            visible="true">
         <run
               class="de.fu_berlin.imp.seqan.usability_analyzer.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="de.fu_berlin.imp.seqan.usability_analyzer.application"
            name="SeqAn Usability Analyzer">
         <property
               name="startupForegroundColor"
               value="000000">
         </property>
         <property
               name="startupProgressRect"
               value="32,268,278,15">
         </property>
         <property
               name="startupMessageRect"
               value="32,288,278,15">
         </property>
      </product>
   </extension>
</plugin>
