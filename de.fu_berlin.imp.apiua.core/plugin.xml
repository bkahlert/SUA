<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.fu_berlin.imp.apiua.core.locatorprovider" name="Locator Provider" schema="schema/de.fu_berlin.imp.apiua.core.locatorprovider.exsd"/>
   <extension-point id="de.fu_berlin.imp.apiua.core.daterange" name="Date Range" schema="schema/de.fu_berlin.imp.apiua.core.daterange.exsd"/>
   <extension-point id="de.fu_berlin.imp.apiua.core.dataload" name="Data Load" schema="schema/de.fu_berlin.imp.apiua.core.dataload.exsd"/>
   <extension point="org.eclipse.ui.views">
      <category id="de.fu_berlin.imp.apiua.category" name="Usability Analysis">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.fu_berlin.imp.apiua.core.preferences.SUACorePreferences"
            id="de.fu_berlin.imp.apiua.core.preferences.SUACorePreferences"
            name="API Usability Analyzer">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.fu_berlin.imp.apiua.core.preferences.SUACorePreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="de.fu_berlin.imp.apiua.category"
            class="de.fu_berlin.imp.apiua.core.views.TimeZoneDateRangeView"
            id="de.fu_berlin.imp.apiua.core.views.TimeZoneDateRangeView"
            name="Date &amp; Time Range"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="de.fu_berlin.imp.apiua.core.services.impl.ServiceFactory">
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.IWorkSessionService"></service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.IDataService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.IHighlightService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.ILabelProviderService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.IUriPresenterService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.location.ILocatorService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.apiua.core.services.IImportanceService">
         </service>
      </serviceFactory>
      <sourceProvider
            provider="de.fu_berlin.imp.apiua.core.preferences.SUACorePreferenceUtil">
         <variable
               name="de.fu_berlin.imp.apiua.core.focused_elements"
               priorityLevel="workbench">
         </variable>
      </sourceProvider>
   </extension>
<extension
         point="org.eclipse.ui.commands">
      <command
            id="de.fu_berlin.imp.apiua.core.commands.startWorkSession"
            name="Start Work Session">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.open"
            name="Open">
      </command>
      <command
            description="Change the data directories that are available."
            id="de.fu_berlin.imp.apiua.core.commands.ChangeDataDirectories"
            name="Add/Remove Data Directories">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.commands.revealInOS"
            name="Show In Finder">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.highlight"
            name="Highlight">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.setImportance"
            name="Set Importance">
         <commandParameter
               id="de.fu_berlin.imp.apiua.core.setImportance.importance"
               name="Importance"
               optional="true"
               values="de.fu_berlin.imp.apiua.core.services.IImportanceService.Importance">
         </commandParameter>
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.commands.showArtefact"
            name="Show Artefact">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.commands.saveFocus"
            name="Save Focus">
      </command>
      <command
            id="de.fu_berlin.imp.apiua.core.commands.loadFocus"
            name="Load Focus">
      </command>
   </extension>
     <extension
           point="org.eclipse.ui.handlers">
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.StartWorkSessionHandler"
              commandId="de.fu_berlin.imp.apiua.core.commands.startWorkSession">
           <activeWhen>
              <and>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.core.consistsOfIWorkSessionEntities">
                 </reference>
              </and>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.ChangeDataDirectories"
              commandId="de.fu_berlin.imp.apiua.core.commands.ChangeDataDirectories">
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.OpenHandler"
              commandId="de.fu_berlin.imp.apiua.core.open">
           <activeWhen>
              <reference
                    definitionId="de.fu_berlin.imp.apiua.core.consistsOfIOpenable">
              </reference>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.RevealInOSHandler"
              commandId="de.fu_berlin.imp.apiua.core.commands.revealInOS">
           <activeWhen>
              <or>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.core.consistsOfIRevealableInOS">
                 </reference>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.core.informationIsIRevealableInOS">
                 </reference>
              </or>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.HighlightHandler"
              commandId="de.fu_berlin.imp.apiua.core.highlight">
           <activeWhen>
              <or>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.core.consistsOfHasDateRanges">
                 </reference>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.core.informationIsHasDateRange">
                 </reference>
              </or>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.SetImportanceHandler"
              commandId="de.fu_berlin.imp.apiua.core.setImportance">
           <activeWhen>
              <reference
                    definitionId="de.fu_berlin.imp.apiua.core.consistsOfURIs">
              </reference>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.ShowArtefactHandler"
              commandId="de.fu_berlin.imp.apiua.core.commands.showArtefact">
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.SaveFocusHandler"
              commandId="de.fu_berlin.imp.apiua.core.commands.saveFocus">
           <activeWhen>
              <or>
                 <reference
                       definitionId="de.fu_berlin.imp.apiua.groundedtheory.isILocatable">
                 </reference>
              </or>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.apiua.core.handlers.LoadFocusHandler"
              commandId="de.fu_berlin.imp.apiua.core.commands.loadFocus">
           <activeWhen>
              <with
                    variable="de.fu_berlin.imp.apiua.core.focused_elements">
                 <count
                       value="+">
                 </count>
              </with>
           </activeWhen>
        </handler>
     </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <separator
               name="de.fu_berlin.imp.apiua.core.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.fu_berlin.imp.apiua.core.commands.startWorkSession"
               disabledIcon="icons/dtool16/worksession_start_tsk.png"
               icon="icons/etool16/worksession_start_tsk.png"
               style="push"
               tooltip="Stop the existing work session and starts a new one">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.hasMultipleIWorkSessionEntities">
                  </reference>
               </and>
            </visibleWhen>
         </command>
         <command
               commandId="de.fu_berlin.imp.apiua.core.commands.saveFocus"
               style="push">
         </command>
         <command
               commandId="de.fu_berlin.imp.apiua.core.open"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.fu_berlin.imp.apiua.core.containsIOpenable">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.rename"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="de.fu_berlin.imp.apiua.core.commands.revealInOS"
               disabledIcon="icons/dtool16/reveal_in_os_tsk.png"
               icon="icons/etool16/reveal_in_os_tsk.png"
               style="push"
               tooltip="Open the selected file in Finder">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.containsIRevealableInOS">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <menu
               commandId="de.fu_berlin.imp.apiua.core.setImportance"
               label="Set Importance">
            <separator
                  name="de.fu_berlin.imp.apiua.core.separator1"
                  visible="true">
            </separator>
            <dynamic
                  class="de.fu_berlin.imp.apiua.core.handlers.SetImportanceContributionItem"
                  id="de.fu_berlin.imp.apiua.core.SetImportanceContributionItem">
            </dynamic>
            <separator
                  name="de.fu_berlin.imp.apiua.core.separator1"
                  visible="true">
            </separator>
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.fu_berlin.imp.apiua.core.containsURIs">
               </reference>
            </visibleWhen>
         </menu>
         <command
               commandId="de.fu_berlin.imp.apiua.core.highlight"
               icon="icons/etool16/mark_occurrences.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.containsHasDateRanges">
                  </reference>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.informationIsHasDateRange">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         
         <separator
               name="de.fu_berlin.imp.apiua.core.separator1"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.bkahlert.nebula.information?after=additions">
         <separator
               name="de.fu_berlin.imp.apiua.core.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.fu_berlin.imp.apiua.core.highlight"
               icon="icons/etool16/mark_occurrences.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.informationIsHasDateRange">
                  </reference>
                  <reference
                        definitionId="de.fu_berlin.imp.apiua.core.containsHasDateRanges">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <separator
               name="de.fu_berlin.imp.apiua.core.separator1"
               visible="true">
         </separator>
      </menuContribution>
  <menuContribution
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               id="de.fu_berlin.imp.apiua.ui.utilities.menu"
               label="Utilities">
            <command
                  commandId="de.fu_berlin.imp.apiua.core.commands.showArtefact"
                  style="push">
            </command>
            <command
                  commandId="de.fu_berlin.imp.apiua.core.commands.saveFocus"
                  style="push">
            </command>
            <command
                  commandId="de.fu_berlin.imp.apiua.core.commands.loadFocus"
                  style="push">
            </command>
            <separator
                  name="de.fu_berlin.imp.apiua.core.commands.viewSeparator"
                  visible="true">
            </separator>
            <dynamic
                  class="de.fu_berlin.imp.apiua.core.ui.ViewMenuContribution"
                  id="de.fu_berlin.imp.apiua.core.ui.ViewMenuContribution">
            </dynamic>
         </menu>
      </menuContribution>
  <menuContribution
        allPopups="false"
        locationURI="toolbar:org.eclipse.ui.main.toolbar">
     <toolbar
           id="de.fu_berlin.imp.apiua.core.main.toolbar">
        <control
              class="de.fu_berlin.imp.apiua.core.SelectionHistoryContribution">
        </control>
     </toolbar>
  </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="de.fu_berlin.imp.apiua.core.definitions.singleSelection">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.definitions.multipleSelections">
         <with
               variable="selection">
            <and>
               <not>
                  <count
                        value="0">
                  </count>
               </not>
               <not>
                  <count
                        value="1">
                  </count>
               </not>
            </and>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.hasMultipleIWorkSessionEntities">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.services.IWorkSessionEntity">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.services.IWorkSessionEntity">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.consistsOfIWorkSessionEntities">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.services.IWorkSessionEntity">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.services.IWorkSessionEntity">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.containsIOpenable">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.model.IOpenable">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.model.IOpenable">
                  </test></or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.consistsOfIOpenable">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.model.IOpenable">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.model.IOpenable">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.containsIRevealableInOS">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.model.IRevealableInOS">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.model.IRevealableInOS">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.consistsOfIRevealableInOS">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.model.IRevealableInOS">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.model.IRevealableInOS">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.informationIsIRevealableInOS">
         <with
               variable="com.bkahlert.nebula.information.input">
            <or>
               <adapt
                     type="de.fu_berlin.imp.apiua.core.model.IRevealableInOS">
               </adapt>
            </or>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.containsHasDateRanges">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.consistsOfHasDateRanges">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.informationIsHasDateRange">
         <with
               variable="com.bkahlert.nebula.information.input">
            <or>
               <adapt
                     type="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
               </adapt>
               <test
                     property="de.fu_berlin.imp.apiua.core.indirectInstanceOf"
                     value="de.fu_berlin.imp.apiua.core.ui.viewer.filters.HasDateRange">
               </test>
            </or>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.containsURIs">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <adapt
                     type="de.fu_berlin.imp.apiua.core.model.URI">
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.consistsOfURIs">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <adapt
                     type="de.fu_berlin.imp.apiua.core.model.URI">
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.apiua.core.informationIsURI">
         <with
               variable="com.bkahlert.nebula.information.input">
            <instanceof
                  value="de.fu_berlin.imp.apiua.core.model.URI">
            </instanceof>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="de.fu_berlin.imp.apiua.core.propertyTesters.URIPropertyTester"
            id="de.fu_berlin.imp.apiua.core.propertyTesters.URIPropertyTester"
            namespace="de.fu_berlin.imp.apiua.core"
            properties="indirectInstanceOf"
            type="de.fu_berlin.imp.apiua.core.model.URI">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            id="de.fu_berlin.imp.apiua.scheme"
            name="API Usability Analyzer"
            parentId="org.eclipse.ui.defaultAcceleratorConfiguration">
      </scheme>
      <key
            commandId="org.eclipse.ui.edit.rename"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="F2">
      </key>
      <key
            commandId="org.eclipse.ui.edit.delete"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="Delete">
      </key>
      <key
            commandId="de.fu_berlin.imp.apiua.core.setImportance"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="M1+1">
         <parameter
               id="de.fu_berlin.imp.apiua.core.setImportance.importance"
               value="DEFAULT">
         </parameter>
      </key>
      <key
            commandId="de.fu_berlin.imp.apiua.core.setImportance"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="M1+2">
         <parameter
               id="de.fu_berlin.imp.apiua.core.setImportance.importance"
               value="LOW">
         </parameter>
      </key>
      <key
            commandId="de.fu_berlin.imp.apiua.core.setImportance"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="M1+3">
         <parameter
               id="de.fu_berlin.imp.apiua.core.setImportance.importance"
               value="HIGH">
         </parameter>
      </key>
      <key
            commandId="de.fu_berlin.imp.apiua.core.commands.saveFocus"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="F5">
      </key>
      <key
            commandId="de.fu_berlin.imp.apiua.core.commands.loadFocus"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.apiua.scheme"
            sequence="F8">
      </key>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.fu_berlin.imp.apiua.core.model.ILocatable"
            class="de.fu_berlin.imp.apiua.core.model.LocatableAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.apiua.core.model.URI">
         </adapter>
      </factory>
      <factory
            adaptableType="de.fu_berlin.imp.apiua.core.model.URI"
            class="de.fu_berlin.imp.apiua.core.model.URIAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.apiua.core.model.ILocatable">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.fu_berlin.imp.apiua.core.Startup">
      </startup>
   </extension>
</plugin>
