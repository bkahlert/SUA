<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource" name="DataSource" schema="schema/de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.fu_berlin.imp.seqan.usability_analyzer.category"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView"
            icon="icons/view16/entity_misc.png"
            id="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView"
            name="Entities">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView">
      </menuContribution>
      
      <menuContribution
            allPopups="false"
            locationURI="toolbar:de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView">
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories"
               icon="IMG_OBJ_FOLDER"
               id="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories"
               style="pulldown">
         </command>
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.DiffOnlyPersonsButton">
         </control>
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.DoclogOnlyPersonsButton">
         </control>
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.SurveyOnlyPersonsButton">
         </control>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories">
         <dynamic
               class="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.DataDirectoriesContributionItem"
               id="de.fu_berlin.imp.seqan.usability_analyzer.entity.dynamic1">
         </dynamic>
         <separator
               name="de.fu_berlin.imp.seqan.usability_analyzer.entity.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories"
               label="Add/Remove Data Directories..."
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.Entity"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.EntityAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.identifier.IIdentifier">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.identifier.ID">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.identifier.Fingerprint">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.identifier.Token">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.survey.model.csv.CSVSurveyRecord">
         </adapter>
      </factory>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.core.daterange">
      <DateRangeListenerFactory
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView$Factory">
      </DateRangeListenerFactory>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.core.dataload">
      <DataLoadProvider
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.EntityDataLoader"
            source="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.IEntityLoadDependency">
         <Dependency
               source="de.fu_berlin.imp.seqan.usability_analyzer.diff.model.IDiffLoadDependency">
         </Dependency>
         <Dependency
               source="de.fu_berlin.imp.seqan.usability_analyzer.doclog.model.IDoclogLoadDependency">
         </Dependency>
         <Dependency
               source="de.fu_berlin.imp.seqan.usability_analyzer.survey.model.ISurveyLoadDependency">
         </Dependency>
      </DataLoadProvider>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource">
      <DataSourceFilterListenerFactory
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView$Factory">
      </DataSourceFilterListenerFactory>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.fu_berlin.imp.seqan.usability_analyzer.core.preferences.SUACorePreferences"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferences"
            id="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferences"
            name="Entities">
      </page>
   </extension>
<extension point="org.eclipse.core.resources.markers" id="com.ibm.mymarkers.mymarker"
  name="My Marker">
        <super type="org.eclipse.core.resources.marker"/>
        <persistent value="false"/>
        <attribute name="description"/>
</extension>
<extension point="org.eclipse.core.resources.markers" id="com.ibm.mymarkers.mymarker"
  name="My Marker">
        <super type="org.eclipse.core.resources.textmarker"/>
        <super type="org.eclipse.core.resources.marker"/>
        <persistent value="true"/>
</extension>
<extension point="org.eclipse.ui.editors.markerAnnotationSpecification" id="myannotationspecification" name="MyAnnotation">
<specification annotationType="com.ibm.example.myannotation" label="MyAnnotation"
                        icon="icons/sample.gif"
                        overviewRulerPreferenceKey="clruler"
                        overviewRulerPreferenceValue="true"
                        colorPreferenceKey="clcolor"
                        colorPreferenceValue="255,255,0"
                        textPreferenceKey="cltext"
                        textPreferenceValue="true"
                        verticalRulerPreferenceKey="clvertical"
                        verticalRulerPreferenceValue="true"
                        textStylePreferenceKey="clstyle"
                        textStylePreferenceValue="BOX">
        </specification>
</extension>
<extension point="org.eclipse.ui.editors.annotationTypes">
                <type markerSeverity="0"
super="org.eclipse.ui.workbench.texteditor.info"
name="com.ibm.example.myannotation" markerType="com.ibm.mymarkers.mymarker"/>
</extension>
<extension
      point="de.fu_berlin.imp.seqan.usability_analyzer.core.locatorprovider">
   <LocatorProviderFactory
         class="de.fu_berlin.imp.seqan.usability_analyzer.entity.gt.EntityLocatorProviderFactory">
   </LocatorProviderFactory>
</extension>
</plugin>
