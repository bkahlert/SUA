<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource" name="DataSource" schema="schema/de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource.exsd"/>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.fu_berlin.imp.seqan.usability_analyzer.category"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView"
            id="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView"
            name="Entities">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="toolbar:de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView">
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.DiffOnlyPersonsButton">
         </control>
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.DoclogOnlyPersonsButton">
         </control>
         <control
               class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.controls.SurveyOnlyPersonsButton">
         </control>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.entity.commands.addCode"
            name="Add Code...">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.handlers.AddCodeHandler"
            commandId="de.fu_berlin.imp.seqan.usability_analyzer.entity.commands.addCode">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.entity.commands.addCode"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.Entity"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.model.EntityAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.ID">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.Fingerprint">
         </adapter>
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.survey.model.SurveyRecord">
         </adapter>
      </factory>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.core.daterange">
      <DateRangeListenerFactory
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView$Factory">
      </DateRangeListenerFactory>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.entity.datasource">
      <DataSourceFilterListenerFactory
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.views.EntityView$Factory">
      </DataSourceFilterListenerFactory>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="de.fu_berlin.imp.seqan.usability_analyzer.core.preferences.SUACorePreferences"
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferences"
            id="de.fu_berlin.imp.seqan.usability_analyzer.entity.preferences.SUAEntityPreferences"
            name="Entities">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.filebuffers.annotationModelCreation">
      <factory
            class="de.fu_berlin.imp.seqan.usability_analyzer.entity.editors.CodeAnnotationModelFactory"
            extensions="*">
      </factory>
   </extension>
   <extension
         id="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code"
         name="Code"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.textmarker">
      </super>
   </extension>
   <extension
         point="org.eclipse.ui.editors.annotationTypes">
      <type
            markerType="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code"
            name="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code">
      </type>
   </extension>
   <extension
         point="org.eclipse.ui.editors.markerAnnotationSpecification">
      <specification
            annotationType="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code"
            colorPreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.color"
            colorPreferenceValue="255,0,0"
            highlightPreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.highlight"
            icon="icons/obj16/code.gif"
            includeOnPreferencePage="true"
            label="Code"
            overviewRulerPreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.overview"
            textPreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.text"
            textStylePreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.textStyle"
            verticalRulerPreferenceKey="de.fu_berlin.imp.seqan.usability_analyzer.entity.markers.code.ruler">
      </specification>
   </extension>

</plugin>
