<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="de.fu_berlin.imp.seqan.usability_analyzer.survey.perspectives.CognitiveDimensionsPerspective"
            id="de.fu_berlin.imp.seqan.usability_analyzer.survey.perspectives.CognitiveDimensionsPerspective"
            name="name">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="de.fu_berlin.imp.seqan.usability_analyzer.category"
            class="de.fu_berlin.imp.seqan.usability_analyzer.survey.views.SurveyView"
            id="de.fu_berlin.imp.seqan.usability_analyzer.survey.views.SurveyView"
            name="Survey">
      </view>
      <view
            category="de.fu_berlin.imp.seqan.usability_analyzer.category"
            class="de.fu_berlin.imp.seqan.usability_analyzer.survey.views.CDView"
            id="de.fu_berlin.imp.seqan.usability_analyzer.survey.views.CDView"
            name="Cognitive Dimensions"
            restorable="true">
      </view>
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.core.dataload">
      <DataLoadProvider
            class="de.fu_berlin.imp.seqan.usability_analyzer.survey.model.SurveyLoader"
            source="de.fu_berlin.imp.seqan.usability_analyzer.survey.model.ISurveyLoadDependency">
      </DataLoadProvider>
   </extension>
   <extension
         point="org.eclipse.ui.propertiesView">
   </extension>
   <extension
         point="de.fu_berlin.imp.seqan.usability_analyzer.core.locatorprovider">
      <LocatorProvider
            class="de.fu_berlin.imp.seqan.usability_analyzer.survey.SurveyLocatorProvider">
      </LocatorProvider>
   </extension>

</plugin>
