         <adapt
               type="de.fu_berlin.imp.seqan.usability_analyzer.groundedtheory.model.ICodeable">
         </adapt>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.groundedtheory.hasICodeables">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <adapt
                     type="de.fu_berlin.imp.seqan.usability_analyzer.groundedtheory.model.ICodeable">
               </adapt>
            </iterate>
