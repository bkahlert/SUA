<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension-point id="de.fu_berlin.imp.seqan.usability_analyzer.core.locatorprovider" name="Locator Provider" schema="schema/de.fu_berlin.imp.seqan.usability_analyzer.core.locatorprovider.exsd"/>
   <extension-point id="de.fu_berlin.imp.seqan.usability_analyzer.core.daterange" name="Date Range" schema="schema/de.fu_berlin.imp.seqan.usability_analyzer.core.daterange.exsd"/>
   <extension-point id="de.fu_berlin.imp.seqan.usability_analyzer.core.dataload" name="Data Load" schema="schema/de.fu_berlin.imp.seqan.usability_analyzer.core.dataload.exsd"/>
   <extension point="org.eclipse.ui.views">
      <category id="de.fu_berlin.imp.seqan.usability_analyzer.category" name="Usability Analysis">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.preferences.SUACorePreferences"
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.preferences.SUACorePreferences"
            name="Seqan Usability Analyzer">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.preferences.SUACorePreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            allowMultiple="false"
            category="de.fu_berlin.imp.seqan.usability_analyzer.category"
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.views.TimeZoneDateRangeView"
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.views.TimeZoneDateRangeView"
            name="Date &amp; Time Range"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.services">
      <serviceFactory
            factoryClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.impl.ServiceFactory">
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IWorkSessionService"></service>
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IDataService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IHighlightService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.ILabelProviderService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IUriPresenterService">
         </service>
         <service
               serviceClass="de.fu_berlin.imp.seqan.usability_analyzer.core.services.location.ILocatorService">
         </service>
      </serviceFactory>
   </extension>
<extension
         point="org.eclipse.ui.commands">
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.startWorkSession"
            name="Start Work Session">
      </command>
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.open"
            name="Open">
      </command>
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.rename"
            name="Rename...">
      </command>
      <command
            description="Change the data directories that are available."
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories"
            name="Add/Remove Data Directories">
      </command>
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.revealInOS"
            name="Show In Finder">
      </command>
      <command
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.highlight"
            name="Highlight">
      </command>
   </extension>
     <extension
           point="org.eclipse.ui.handlers">
        <handler
              class="de.fu_berlin.imp.seqan.usability_analyzer.core.handlers.StartWorkSessionHandler"
              commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.startWorkSession">
           <activeWhen>
              <and>
                 <reference
                       definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIWorkSessionEntities">
                 </reference>
              </and>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.seqan.usability_analyzer.core.handlers.ChangeDataDirectories"
              commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.ChangeDataDirectories">
        </handler>
        <handler
              class="de.fu_berlin.imp.seqan.usability_analyzer.core.handlers.OpenHandler"
              commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.open">
           <activeWhen>
              <reference
                    definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIOpenable">
              </reference>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.seqan.usability_analyzer.core.handlers.RevealInOSHandler"
              commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.revealInOS">
           <activeWhen>
              <reference
                    definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIRevealableInOS">
              </reference>
           </activeWhen>
        </handler>
        <handler
              class="de.fu_berlin.imp.seqan.usability_analyzer.core.handlers.HighlightHandler"
              commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.highlight">
           <activeWhen>
              <or>
                 <reference
                       definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfHasDateRanges">
                 </reference>
                 <reference
                       definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.informationIsHasDateRange">
                 </reference>
              </or>
           </activeWhen>
        </handler>
     </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="popup:org.eclipse.ui.popup.any?before=additions">
         <separator
               name="de.fu_berlin.imp.seqan.usability_analyzer.core.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.startWorkSession"
               disabledIcon="icons/dtool16/worksession_start_tsk.png"
               icon="icons/etool16/worksession_start_tsk.png"
               style="push"
               tooltip="Stop the existing work session and starts a new one">
            <visibleWhen
                  checkEnabled="false">
               <and>
                  <reference
                        definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.hasMultipleIWorkSessionEntities">
                  </reference>
               </and>
            </visibleWhen>
         </command>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.open"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.containsIOpenable">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.edit.rename"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.commands.revealInOS"
               disabledIcon="icons/dtool16/reveal_in_os_tsk.png"
               icon="icons/etool16/reveal_in_os_tsk.png"
               style="push"
               tooltip="Open the selected file in Finder">
            <visibleWhen
                  checkEnabled="false">
               <reference
                     definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.containsIRevealableInOS">
               </reference>
            </visibleWhen>
         </command>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.highlight"
               icon="icons/etool16/mark_occurrences.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.informationIsHasDateRange">
                  </reference>
                  <reference
                        definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.containsHasDateRanges">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         
         <separator
               name="de.fu_berlin.imp.seqan.usability_analyzer.core.separator1"
               visible="true">
         </separator>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:com.bkahlert.nebula.information?after=additions">
         <separator
               name="de.fu_berlin.imp.seqan.usability_analyzer.core.separator1"
               visible="true">
         </separator>
         <command
               commandId="de.fu_berlin.imp.seqan.usability_analyzer.core.highlight"
               icon="icons/etool16/mark_occurrences.png"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <reference
                        definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.informationIsHasDateRange">
                  </reference>
                  <reference
                        definitionId="de.fu_berlin.imp.seqan.usability_analyzer.core.containsHasDateRanges">
                  </reference>
               </or>
            </visibleWhen>
         </command>
         <separator
               name="de.fu_berlin.imp.seqan.usability_analyzer.core.separator1"
               visible="true">
         </separator>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.definitions.singleSelection">
         <with
               variable="selection">
            <count
                  value="1">
            </count>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.definitions.multipleSelections">
         <with
               variable="selection">
            <and>
               <not>
                  <count
                        value="0">
                  </count>
               </not>
               <not>
                  <count
                        value="1">
                  </count>
               </not>
            </and>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.hasMultipleIWorkSessionEntities">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IWorkSessionEntity">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IWorkSessionEntity">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIWorkSessionEntities">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IWorkSessionEntity">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.services.IWorkSessionEntity">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.containsIOpenable">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IOpenable">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IOpenable">
                  </test></or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIOpenable">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IOpenable">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IOpenable">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.containsIRevealableInOS">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IRevealableInOS">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IRevealableInOS">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfIRevealableInOS">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IRevealableInOS">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IRevealableInOS">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.containsHasDateRanges">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="or">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfHasDateRanges">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <adapt
                        type="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
                  </adapt>
                  <test
                        property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                        value="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
                  </test>
               </or>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.informationIsHasDateRange">
         <with
               variable="com.bkahlert.nebula.information.input">
            <or>
               <adapt
                     type="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
               </adapt>
               <test
                     property="de.fu_berlin.imp.seqan.usability_analyzer.core.indirectInstanceOf"
                     value="de.fu_berlin.imp.seqan.usability_analyzer.core.ui.viewer.filters.HasDateRange">
               </test>
            </or>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.consistsOfURIs">
         <with
               variable="selection">
            <iterate
                  ifEmpty="false"
                  operator="and">
               <adapt
                     type="java.net.URI">
               </adapt>
            </iterate>
         </with>
      </definition>
      <definition
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.informationIsURI">
         <with
               variable="com.bkahlert.nebula.information.input">
            <instanceof
                  value="java.net.URI">
            </instanceof>
         </with>
      </definition>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.propertyTesters.URIPropertyTester"
            id="de.fu_berlin.imp.seqan.usability_analyzer.core.propertyTesters.URIPropertyTester"
            namespace="de.fu_berlin.imp.seqan.usability_analyzer.core"
            properties="indirectInstanceOf"
            type="java.net.URI">
      </propertyTester>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <scheme
            id="de.fu_berlin.imp.seqan.usability_analyzer.scheme"
            name="SeqAn Usability Analyzer">
      </scheme>
      <key
            commandId="org.eclipse.ui.edit.rename"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.seqan.usability_analyzer.scheme"
            sequence="F2">
      </key>
      <key
            commandId="org.eclipse.ui.edit.delete"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="de.fu_berlin.imp.seqan.usability_analyzer.scheme"
            sequence="Delete">
      </key>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="de.fu_berlin.imp.seqan.usability_analyzer.core.model.data.IData"
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.model.data.impl.DataAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.IRevealableInOS">
         </adapter>
      </factory>
      <factory
            adaptableType="de.fu_berlin.imp.seqan.usability_analyzer.core.model.ILocatable"
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.model.LocatableAdapterFactory">
         <adapter
               type="java.net.URI">
         </adapter>
      </factory>
      <factory
            adaptableType="java.net.URI"
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.model.URIAdapterFactory">
         <adapter
               type="de.fu_berlin.imp.seqan.usability_analyzer.core.model.ILocatable">
         </adapter>
      </factory>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="de.fu_berlin.imp.seqan.usability_analyzer.core.Startup">
      </startup>
   </extension>
</plugin>
